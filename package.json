{
  "name": "sptools",
  "version": "1.0.0",
  "description": "This is the 1st version of SPtools website",
  "main": "home.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "clean": "rimraf dist",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
    "usemin": "usemin home.html -d dist -o dist/home.html --htmlmin true",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonusahu05/SPtools.git"
  },
  "author": "Sonu Sahu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sonusahu05/SPtools/issues"
  },
  "homepage": "https://github.com/sonusahu05/SPtools#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "mdb-ui-kit": "^4.2.0",
    "popper.js": "^1.16.1"
  }
}
